__pycache__/
*.py[cod]
*$py.class
*.so
.Python
*.egg-info/
dist/
build/
*.egg
.pytest_cache/
.coverage
htmlcov/
.tox/
.mypy_cache/
.dmypy.json
dmypy.json

# Virtual environments
venv/
env/
ENV/
.venv

# IDE
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Git
.git/
.gitignore
.gitattributes

# CI/CD
.github/
.gitlab-ci.yml
azure-pipelines.yml

# Documentation (not needed in runtime)
docs/
*.md
!README.md
LICENSE

# Tests
tests/
test_*.py
*_test.py
pytest.ini
.pytest_cache/

# Notebooks
notebooks/
*.ipynb
.ipynb_checkpoints/

